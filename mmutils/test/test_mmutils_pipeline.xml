<pipeline>
    <docstring>
        mmutils - Test pipeline
    </docstring>
    <units>
        <unit name="element_to_list">
            <module>mmutils.io.element_to_list</module>
            <set name="output_directory" value="Undefined"/>
        </unit>
        <unit name="list_to_element">
            <module>mmutils.io.list_to_element</module>
        </unit>
        <unit name="compress">
            <module>mmutils.io.gzip_file</module>
        </unit>
        <unit name="decompress">
            <module>mmutils.io.ungzip_file</module>
        </unit>
    </units>
    <links>
        <!-- INPUTS -->
        <link source="input_value" destination="element_to_list.element"/>
        <link source="input_file" destination="compress.fname"/>
        <!-- LINKS -->
        <link source="element_to_list.adaptedelement" destination="list_to_element.listobj"/>
        <link source="compress.gzipfname" destination="decompress.fname"/>
        <!-- OUTPUTS -->
        <link source="decompress.ungzipfname" destination="out_file"/>
        <link source="list_to_element.element" destination="out_element"/>
    </links>
    <positions>
        <position unit="inputs" x="-766" y="224"/>
        <position unit="outputs" x="526" y="-19"/>
    </positions>
    <scale factor="0.5"/>
</pipeline>
